                                    </t>
                                    <t t-elif="state.selectedModel == 'product.public.category'">
                                        <img t-attf-src="/web/image/product.public.category/#{record.id}/image_1920/300x300" width="138px" height="138px" class="object-cover"/>
                                    </t>
                                    <t t-else="">
                                        <!-- <img t-attf-src="/web/image/blog.post/#{record.id}/image_128" /> -->
                                        <div t-att-style="'background-image:' + record?.cover_properties + '; background-size: cover; background-position: center; background-repeat: no-repeat; width : 300px; height : 200px; z-index : 0; border-radius : 10px 10px 0 0'">
                                        </div>
                                    </t>
                                    <span ><t t-esc="record.name.slice(0,8)"/>...</span>
                                    <i style="cursor-pointer" class="btn-remove fa fa-remove" t-on-click="ev => this.deleteRecord(record_index)"/>
                                </div>
                            </t>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
</templates>
